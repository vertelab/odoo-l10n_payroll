<odoo>
<data>
    <record id="res_config_settings_url_view_form" model="ir.ui.view">
        <field name="name">res.config.settings.url.view.form</field>
        <field name="model">res.config.settings</field>
        <field name="priority" eval="41"/>
        <field name="inherit_id" ref="ics_import_holidays.res_config_settings_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='ics_url']" position="replace">
                <field name="ics_url_eve"></field>
            </xpath>
        </field>
    </record>
</data>
</odoo>
